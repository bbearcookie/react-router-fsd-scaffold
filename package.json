{
  "name": "react-router-fsd-scaffold",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@10.22.0",
  "scripts": {
    "postinstall": "lefthook install",
    "dev": "react-router dev",
    "dev:msw": "cross-env VITE_MSW=true react-router dev",
    "storybook": "storybook dev -p 6006",
    "build": "react-router typegen && tsc -b && react-router build",
    "build:storybook": "storybook build",
    "start": "react-router-serve ./dist/server/index.js",
    "preview": "vite preview --outDir dist/client",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prettier:write": "prettier --write",
    "test": "vitest",
    "test:e2e": "playwright test",
    "generate:api": "orval --config orval.config.js"
  },
  "dependencies": {
    "@react-router/node": "^7.12.0",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-query": "^5.90.19",
    "@tanstack/react-query-devtools": "^5.91.2",
    "es-toolkit": "^1.44.0",
    "i18next": "^25.7.4",
    "i18next-resources-to-backend": "^1.2.1",
    "isbot": "^5",
    "ky": "^1.14.2",
    "nuqs": "^2.8.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-i18next": "^16.5.3",
    "react-router": "^7.12.0",
    "tailwindcss": "^4.1.18",
    "zod": "^4.3.5"
